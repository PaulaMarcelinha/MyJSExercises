// Count the number of divisors of a positive integer n.


function getDivisorsCnt(n){
    // todo
  let nDivisors = 0;
  
    for (let i = 0; i <= n; i++) {
      if(n % i === 0) {
        nDivisors++;
      }
    }
  return nDivisors;
}

console.log(getDivisorsCnt(1));
console.log(getDivisorsCnt(10));
console.log(getDivisorsCnt(5));
console.log(getDivisorsCnt(54));
